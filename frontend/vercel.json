{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/sw.js",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/sw.js"
    },
    {
      "src": "/registerSW.js",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/registerSW.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
